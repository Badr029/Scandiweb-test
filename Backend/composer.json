{
    "name": "scandiweb/fullstack",
    "type": "project",
    "description": "Scandiweb Fullstack Test Backend - Pure PHP with GraphQL",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "nikic/fast-route": "^1.3",
        "webonyx/graphql-php": "^15.2",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
